{
  "name": "orgchart-pro",
  "homepage": "https://sasuhai.github.io/Orgchart/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:css": "tailwindcss -i ./src/orgchart.css -o ./src/output.css --watch",
